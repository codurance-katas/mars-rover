    agent any

    stages {
        stage('Build') {
            steps {
                echo "I'm building, my friend"
            }
        }

        stage('Testing') {
            steps {
                echo "I'm testing, my lord"
            }
        }

        stage('Deploy') {
            steps {
                echo "Here's is your artifact"
            }
        }
    }
}
